{
  "expo": {
    "name": "HackChat",
    "slug": "hack",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/adaptive.png",
    "splash": {
      "image": "./assets/splashcentered.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hackchat.hack"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive.png",
        "backgroundColor": "#000000"
      },
      "permissions": [
        "READ_CONTACTS",
        "WRITE_CONTACTS",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.hackchat.hack"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
